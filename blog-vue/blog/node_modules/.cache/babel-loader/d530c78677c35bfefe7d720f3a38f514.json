er from \"./components/zw-player/player.vue\";\nimport ChatRoom from \"./components/ChatRoom\";\nexport default {\n  created() {\n    // 获取博客信息\n    this.getBlogInfo();\n    // 上传访客信息\n    this.axios.post(\"/api/report\");\n  },\n  components: {\n    TopNavBar,\n    Player,\n    SideNavBar,\n    Footer,\n    BackTop,\n    searchModel,\n    LoginModel,\n    RegisterModel,\n    ForgetModel,\n    EmailModel,\n    ChatRoom\n  },\n  methods: {\n    getBlogInfo() {\n      this.axios.get(\"/api/\").then(({ data }) => {\n        this.$store.commit(\"checkBlogInfo\", data.data);\n      });\n    }\n  },\n  computed: {\n    blogInfo() {\n      return this.$store.state.blogInfo;\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src"}]}